{
  "config": {
    "WP_DEBUG_LOG": "/var/www/html/wp-content/debug.log"
  },
  "env": {
    "tests": {
      "mysqlPort": 33066
    }
  },
  "lifecycleScripts": {
    "afterStart": "bash 'tests/wp-env/initialize-external.sh'"
  },
  "mappings": {
    "setup": "./tests/wp-env",
